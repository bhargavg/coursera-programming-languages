#!/usr/bin/env bash

# Usage: 
#   $ ./bin/run_tests WEEK_NUMBER
# Example:
#   $ ./bin/run_tests 2    # to run week 2 tests


if [ -z "$1" ]
then
    echo "Usage: "
    echo "./bin/run_tests WEEK_NUMBER"
    echo ""
    echo "Example: "
    echo "./bin/run_tests 2"
    exit 1
fi

sml <"PartA/Assignments/0$1_Week_tests.sml"
